{
  "name": "testudoq",
  "version": "2.0.0",
  "scripts": {
    "lint": "eslint src test",
    "pretest": "npm run lint && rimraf testem/compiled",
    "test": "testem ci -R dot",
    "pretest-browser": "npm run pretest",
    "test-browser": "testem",
    "pretest-chrome": "npm run pretest",
    "test-chrome": "testem -l Chrome",
    "sourcemap": "sourcemap-lookup",
    "prepack-extension": "rimraf pack && copyfiles -u 1 template/**/* pack",
    "pack-extension": "webpack --config webpack.config.js",
    "build-min-obs-extension": "npm run prepack-extension && cross-env DELAY=2 node -e \"setTimeout(() => {}, process.env.DELAY * 1000)\" && npm run pack-extension && npm run post-build-min-obs",
    "build-extension": "npm run prepack-extension && cross-env DELAY=2 node -e \"setTimeout(() => {}, process.env.DELAY * 1000)\" && npm run pack-extension",
    "ff-build": "web-ext build",
    "post-build-min-obs": "node post-build-min-obs"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-crockford": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "fs-readdir-recursive": "^1.1.0",
    "html-loader": "^5.0.0",
    "jasmine": "^5.1.0",
    "jest": "^29.7.0",
    "sourcemap-lookup": "0.0.5",
    "testem": "^3.11.0",
    "uglify-js": "^3.17.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "javascript-obfuscator": "^4.1.0",
    "ncp": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5"
  }
}
